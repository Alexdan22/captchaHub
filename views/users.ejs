    <link rel="stylesheet" type="text/css" href="../assets/css/app.css">
    <!-- END MODERN CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="../assets/css/core/menu/menu-types/vertical-compact-menu.css">
    <link rel="stylesheet" type="text/css" href="../assets/vendors/css/cryptocoins/cryptocoins.css">
    <link rel="stylesheet" type="text/css" href="../assets/css/pages/wallet.css">
    <!-- END Page Level CSS-->
    <!-- BEGIN Custom CSS-->
    <link rel="stylesheet" type="text/css" href="../assets/css/style.css">
    <!-- END Custom CSS-->
  </head>
  <body class="vertical-layout vertical-compact-menu content-detached-right-sidebar   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-compact-menu" data-col="content-detached-right-sidebar">

    <!-- fixed-top-->
    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow fixed-top navbar-light navbar-bg-color">
        <div class="navbar-wrapper">
          <div class="navbar-header d-md-none">
            <ul class="nav navbar-nav flex-row">
              <li class="nav-item mobile-menu d-md-none mr-auto"><a class="nav-link nav-menu-main menu-toggle hidden-xs" href="#"><i class="ft-menu font-large-1"></i></a></li>
              <li class="nav-item d-md-none"><a class="navbar-brand" href="/admin"><img class="brand-logo d-none d-md-block" alt="CryptoDash admin logo" src="../assets/images/logo/logo.png"><img class="brand-logo d-sm-block d-md-none" alt="CryptoDash admin logo sm" src="../assets/images/logo/logo-sm.png"></a></li>
              </ul>
          </div>
        </div>
      </nav>
  
      <!-- ////////////////////////////////////////////////////////////////////////////-->
  
  
      <div class="main-menu menu-fixed menu-dark menu-bg-default rounded menu-accordion menu-shadow">
        <div class="main-menu-content"><a class="navigation-brand d-none d-md-block d-lg-block d-xl-block" href="/admin"><img class="brand-logo" src="../assets/images/logo/logo.png"/></a>
          <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
            <li class=" nav-item"><a href="/admin"><i class="icon-grid"></i><span class="menu-title" data-i18n="">Dashboard</span></a>
            </li>
            <li class="nav-item"><a href=""><i class="icon-user-following"></i><span class="menu-title" data-i18n="">Profile</span></a>
            </li>
            <li class="nav-item"><a href=""><i class="icon-wallet"></i><span class="menu-title" data-i18n="">Join today</span></a>
            </li>
            <li class="active"><a href=""><i class="icon-shuffle"></i><span class="menu-title" data-i18n="">Transactions</span></a>
            </li>
            <li class=" nav-item mt-3"><a href=/log-out"><span class="menu-title" data-i18n="">Log out</span></a>
            </li>
          </ul>
        </div>
      </div>

    <div class="app-content content">
      <div class="content-wrapper">
        <div class="content-header row">
          <div class="content-header-left col-md-8 col-12 mb-2 breadcrumb-new">
            <h3 class="content-header-title mb-0 d-inline-block">User Details</h3>
            <div class="row breadcrumbs-top d-inline-block">
              <div class="breadcrumb-wrapper col-12">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="/admin">Admin panel</a>
                  </li>
                  <li class="breadcrumb-item active">User details
                  </li>
                </ol>
              </div>
            </div>
          </div>
        </div>

                <%if(activeUsers.length != 0){%>
        <div class="row">
            <div id="recent-transactions" class="col-12">
                <div class="card">
                    <div class="card-content">
                        <div class="table-responsive">
                            <table id="recent-orders" class="table table-hover table-xl mb-0">
                                <thead>
                                    <tr>
                                        <th class="border-top-0">Email</th>
                                        <th class="border-top-0">User ID</th>
                                        <th class="border-top-0">Sponsor ID</th>
                                        <th class="border-top-0">Package</th>
                                        <th class="border-top-0">Total Earnings</th>
                                        <th class="border-top-0">Available Balance</th>
                                        <th class="border-top-0">View profile</th>
                                        <th class="border-top-0">Date</th>
                                        <th class="border-top-0">Password</th>
                                    </tr>
                                </thead>
                                <tbody>
                    <%activeUsers.forEach(function(user){%>
                        <%if(user.status == 'Active'){%>
                                        <tr>
                                            <td class="text-truncate"><i class="la la-dot-circle-o success font-medium-1 mr-1"></i> <%=user.email%></td>
                                            <td class="text-truncate p-1"><%=user.userID%></td>
                                            <td class="text-truncate p-1"><%=user.sponsorID%></td>
                                            <td><%=user.package.stage%></td>
                                            <td><%=user.earnings.total%></td>
                                            <td><%=user.earnings.balance%></td>
                                            <td class="text-truncate">
                                                <a href="/viewUser/<%=user.email%>" class="mb-0 btn-sm btn btn-outline-warning round">View profile</a>
                                            </td>
                                            <td class="text-truncate"><a href="#"><%=user.time.date%>/<%=user.time.month%>/<%=user.time.year%></a></td>
                                            <td class="text-truncate p-1"><%=user.password%></td>
                                        </tr>
                        <%}else{%>
                                        <tr>
                                            <td class="text-truncate"><i class="la la-dot-circle-o warning font-medium-1 mr-1"></i> <%=user.email%></td>
                                            <td class="text-truncate p-1"><%=user.userID%></td>
                                            <td class="text-truncate p-1"><%=user.sponsorID%></td>
                                            <td>Nil</td>
                                            <td><%=user.earnings.total%></td>
                                            <td><%=user.earnings.balance%></td>
                                            <td class="text-truncate">
                                                <a href="/viewUser/<%=user.email%>" class="mb-0 btn-sm btn btn-outline-warning round">View profile</a>
                                            </td>
                                            <td class="text-truncate"><a href="#"><%=user.time.date%>/<%=user.time.month%>/<%=user.time.year%></a></td>
                                            <td class="text-truncate p-1"><%=user.password%></td>
                                        </tr>
                        <%}%>
                   <% });%>
        
                                </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    <%}else{%>
        
        <br><br>
        <div class="">
            <h6 class="mb-1 text-dark text-sm">No Users found</h6>
        </div>

    <%}%>
            
                
      </div>
    </div>
    <!-- ////////////////////////////////////////////////////////////////////////////-->


    <footer class="footer footer-static footer-transparent">
      <p class="clearfix blue-grey lighten-2 text-sm-center mb-0 px-2"><span class="float-md-left d-block d-md-inline-block">Copyright  &copy; <script>
          document.write(new Date().getFullYear())
          </script>, All rights reserved. </span></p>
    </footer>

    <!-- BEGIN VENDOR JS-->
    <script src="../assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN MODERN JS-->
    <script src="../assets/js/core/app-menu.js" type="text/javascript"></script>
    <script src="../assets/js/core/app.js" type="text/javascript"></script>
    <script src="../assets/js/response.js" type="text/javascript"></script>
    <script src="../assets/js/request.js" type="text/javascript"></script>
    <!-- END MODERN JS-->
    <!-- BEGIN PAGE LEVEL JS-->
    <!-- END PAGE LEVEL JS-->
  </body>
</html>