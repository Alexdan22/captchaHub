    <div class="app-content content">
      <div class="content-wrapper">
        <div class="content-header row">
          <div class="content-header-left col-md-8 col-12 mb-2 breadcrumb-new">
            <h3 class="content-header-title mb-0 d-inline-block">Account Profile</h3>
            <div class="row breadcrumbs-top d-inline-block">
              <div class="breadcrumb-wrapper col-12">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="/dashboard">Dashboard</a>
                  </li>
                  <li class="breadcrumb-item active">Account Profile
                  </li>
                </ol>
              </div>
            </div>
          </div>
        </div>
        <div class="content-body"><div class="row">
    <div class="col-12 col-md-8">
        <!-- User Profile -->
        <section class="card">
            <div class="card-content">
                <div class="card-body">
                    <div class="col-12">
                        <div class="row">
                            <div class="col-md-2 col-12">
                                <img src="../assets/images/portrait/medium/avatar-m-1.png" class="rounded-circle height-100" alt="Card image" />
                            </div>
                            <div class="col-md-10 col-12">
                                <div class="row">
                                    <div class="col-12 col-md-4">
                                        <p class="text-bold-700 text-uppercase mb-0">Username</p>
                                        <p class="mb-0"><%=username%></p>
                                    </div>
                                    <div class="col-12 col-md-4">
                                        <p class="text-bold-700 text-uppercase mb-0">Email</p>
                                        <p class="mb-0"><%=email%></p>
                                    </div>
                                    <%if(mobile == 0){%>
                                        <div class="col-12 col-md-4">
                                            <p class="text-bold-700 text-uppercase mb-0">Mobile number</p>
                                            <p class="mb-0">Not provided</p>
                                        </div>
                                    <%}else{%>
                                        <div class="col-12 col-md-4">
                                            <p class="text-bold-700 text-uppercase mb-0">Mobile number</p>
                                            <p class="mb-0">+91 <%=mobile%></p>
                                        </div>
                                    <%}%>
                                    <div class="col-12 col-md-4">
                                        <p class="text-bold-700 text-uppercase mb-0">User ID</p>
                                        <p class="mb-0"><%=userID%></p>
                                    </div>
                                    <div class="col-12 col-md-4">
                                        <p class="text-bold-700 text-uppercase mb-0">Sponsor ID</p>
                                        <p class="mb-0"><%=sponsorID%></p>
                                    </div>
                                    <div class="col-12 col-md-4">
                                        <p class="text-bold-700 text-uppercase mb-0">Status</p>
                                        <p class="mb-0"><%=status%></p>
                                    </div>
                                </div>
                                <hr/>
                                <%if(bank == 'Not provided'){%>
                                    <form class="form-horizontal form-user-profile row mt-2">
                                        <div class="col-12">
                                            <p>Bank details</p>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="holdersName" id="holdersName"  required="" autofocus="">
                                                <label for="holdersName"> Holder's name</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="number" class="form-control" name="accountNumber" id="accountNumber"  required="" autofocus="">
                                                <label for="accountNumber">Account Number</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="bankName" id="bankName"  required="" autofocus="">
                                                <label for="bankName">Bank name</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="ifsc" id="ifsc" required="" autofocus="">
                                                <label for="ifsc">IFSC Code</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-12 text-right">
                                            <button type="button" onclick="bankDetails()" class="btn-gradient-primary my-1">Save</button>
                                        </div>
                                    </form>
                                    <div id="alert">
                                      </div>
                                <%}else{%>
                                    <form class="form-horizontal form-user-profile row mt-2">
                                        <div class="col-12">
                                            <p>Bank details</p>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="holdersName" disabled value="<%=bankDetails.name%>" id="holdersName"  required="" autofocus="">
                                                <label for="holdersName"> Holder's name</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="number" class="form-control" name="accountNumber" disabled value="<%=bankDetails.accountNumber%>" id="accountNumber"  required="" autofocus="">
                                                <label for="accountNumber">Account Number</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="bankName" disabled value="<%=bankDetails.bankName%>" id="bankName"  required="" autofocus="">
                                                <label for="bankName">Bank name</label>
                                            </fieldset>
                                        </div>
                                        <div class="col-6">
                                            <fieldset class="form-label-group">
                                                <input type="text" class="form-control" name="ifsc" disabled value="<%=bankDetails.ifsc%>" id="ifsc" required="" autofocus="">
                                                <label for="ifsc">IFSC Code</label>
                                            </fieldset>
                                        </div>
                                    </form>
                                <%}%>
                                <h5>Referral Link</h5>
                                <hr/>
                                <form class="form-horizontal form-referral-link row mt-2" action="">
                                    <div class="col-12">
                                        <fieldset class="form-label-group">
                                            <input type="text" class="form-control" id="referral-link" value="https://mycaptchahub.in/register/<%=userID%>" required="" autofocus="">
                                            <label for="first-name">Referral link</label>
                                            <a href="https://api.whatsapp.com/send?text=https://mycaptchahub.in/register/<%= userID %>" class="btn btn-warning round mr-1 mt-1 mb-0">Share</a>
                                        </fieldset>
                                    </div>
                                </form>
                                <p>This is your Captcha Hub referral link. You can use it to share with your friends and other interested parties. If any of them sign up with this link, they will get reward amounts to 10% and will be added to your referral program.</p>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>
        </div>
      </div>
    </div>
    <!-- ////////////////////////////////////////////////////////////////////////////-->


